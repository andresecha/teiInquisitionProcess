   </xs:attributeGroup>
   <xs:attributeGroup name="tei_inq_att.milestoneUnit.attribute.unit">
      <xs:attribute name="unit" use="required">
         <xs:annotation>
            <xs:documentation>proporciona un nombre convencional para el tipo de sección que cambia en este hito.
Suggested values include: 1] page; 2] column; 3] line; 4] book; 5] poem; 6] canto; 7] speaker; 8] stanza; 9] act; 10] scene; 11] section; 12] absent; 13] unnumbered</xs:documentation>
         </xs:annotation>
         <xs:simpleType>
            <xs:union>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="page"/>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="column">
                        <xs:annotation>
                           <xs:documentation>cambio de columna.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="line"/>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="book">
                        <xs:annotation>
                           <xs:documentation>cualquier unidad llamada libro, pliego, etc.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="poem">
                        <xs:annotation>
                           <xs:documentation>poemas individuales de una colección.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="canto">
                        <xs:annotation>
                           <xs:documentation>cantos u otras secciones importantes de un poema.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="speaker">
                        <xs:annotation>
                           <xs:documentation>changes of speaker or narrator.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="stanza">
                        <xs:annotation>
                           <xs:documentation>estrofas dentro de un poema, de un libro, o de un canto.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="act">
                        <xs:annotation>
                           <xs:documentation>actos dentro de un drama.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="scene">
                        <xs:annotation>
                           <xs:documentation>escenas dentro de un drama o de un acto.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="section">
                        <xs:annotation>
                           <xs:documentation>secciones de cualquier tipo.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="absent">
                        <xs:annotation>
                           <xs:documentation>pasajes no presentes en la edición de referencia.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:enumeration value="unnumbered">
                        <xs:annotation>
                           <xs:documentation>passages present in the text, but not to be included as part of the reference.</xs:documentation>
                        </xs:annotation>
                     </xs:enumeration>
                  </xs:restriction>
               </xs:simpleType>
               <xs:simpleType>
                  <xs:restriction base="xs:token">
                     <xs:pattern value="[^\p{C}\p{Z}]+"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:union>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
