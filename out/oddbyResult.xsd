   <xs:element name="relation">
      <xs:annotation>
         <xs:documentation>(relación) describe cualquier tipo de relación o vínculo entre un determinato grupo de personas. [14.3.2.3. Personal Relationships]</xs:documentation>
      </xs:annotation>
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="tei:desc"/>
         </xs:sequence>
         <xs:attributeGroup ref="tei:tei_inq_att.global.attribute.xmlid"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.attribute.n"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.attribute.xmllang"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.attribute.xmlbase"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.attribute.xmlspace"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.rendition.attribute.rend"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.rendition.attribute.style"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.rendition.attribute.rendition"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.change.attribute.change"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.responsibility.attribute.cert"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.responsibility.attribute.resp"/>
         <xs:attributeGroup ref="tei:tei_inq_att.global.source.attribute.source"/>
         <xs:attributeGroup ref="tei:tei_inq_att.datable.iso.attribute.when-iso"/>
         <xs:attributeGroup ref="tei:tei_inq_att.datable.iso.attribute.notBefore-iso"/>
         <xs:attributeGroup ref="tei:tei_inq_att.datable.iso.attribute.notAfter-iso"/>
         <xs:attributeGroup ref="tei:tei_inq_att.datable.iso.attribute.from-iso"/>
         <xs:attributeGroup ref="tei:tei_inq_att.datable.iso.attribute.to-iso"/>
         <xs:attributeGroup ref="tei:tei_inq_att.editLike.attributes"/>
         <xs:attributeGroup ref="tei:tei_inq_att.sortable.attributes"/>
         <xs:attribute name="type">
            <xs:annotation>
               <xs:documentation>(Tipo de la relación) A seleccionar en una lista cerrada</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:token">
                  <xs:enumeration value="familiar">
                     <xs:annotation>
                        <xs:documentation>(Relaciones familiares) Atributo empleado para clasificar el conjunto total de las relaciones familiares compiladas</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="servidumbre">
                     <xs:annotation>
                        <xs:documentation>(Relaciones de servicio) Atributo empleado para clasificar el conjunto total de las relaciones entre amos y siervos compiladas</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="vecindad">
                     <xs:annotation>
                        <xs:documentation>(Relación de personas con sus lugares de vecindad) Atributo empleado para clasificar el conjunto total de las relaciones de vecindad de personas con entidades territorial</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="magisterio">
                     <xs:annotation>
                        <xs:documentation>(Relación de maestros con discípulos) Atributo empleado para clasificar el conjunto total de las relaciones entre maestros y discipulos compiladas</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="active">
            <xs:annotation>
               <xs:documentation>identifica los participantes activos en una relación no-mutua, o todos los participantes de una mutua.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction>
                  <xs:simpleType>
                     <xs:list>
                        <xs:simpleType>
                           <xs:restriction base="xs:anyURI">
                              <xs:pattern value="\S+"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:list>
                  </xs:simpleType>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="mutual">
            <xs:annotation>
               <xs:documentation>proporciona una lista de participantes entre los cuales se establece una idéntica relación</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction>
                  <xs:simpleType>
                     <xs:list>
                        <xs:simpleType>
                           <xs:restriction base="xs:anyURI">
                              <xs:pattern value="\S+"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:list>
                  </xs:simpleType>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
         <xs:attribute name="name">
            <xs:annotation>
               <xs:documentation>(Nombre de la relación) A seleccionar en una lista cerrada</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:token">
                  <xs:enumeration value="amos">
                     <xs:annotation>
                        <xs:documentation>(Relación de amos a siervos) Relación dirigida a anotar en un solo sentido</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="siervos">
                     <xs:annotation>
                        <xs:documentation>(Relación de siervos a amos) Relación dirigida a anotar en un solo sentido</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="padres">
                     <xs:annotation>
                        <xs:documentation>(Relación de padres a hijos) Relación dirigida a anotar en un solo sentido</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="padres_naturales">
                     <xs:annotation>
                        <xs:documentation>(Relación de padres naturales a hijos naturales) Relación dirigida a anotar en un solo sentido</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="hijos">
                     <xs:annotation>
                        <xs:documentation>(Relación de hijos a padres) Relación dirigida a anotar en un solo sentido</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="hijos_naturales">
                     <xs:annotation>
                        <xs:documentation>(Relación de hijos naturales a padres naturales) Relación dirigida a anotar en un solo sentido</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="tios">
                     <xs:annotation>
                        <xs:documentation>(Relación de tíos a sobrinos) Relación dirigida a anotar en un solo sentido. NO anotar en caso de que las relaciones de paternidad que ligan los nodos existan</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="sobrinos">
                     <xs:annotation>
                        <xs:documentation>(Relación de sobrinos a tíos) Relación dirigida a anotar en un solo sentido. NO anotar en caso de que las relaciones de paternidad que ligan los nodos existan</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="descendientes_bastardos">
                     <xs:annotation>
                        <xs:documentation>(Relación de descendientes a ascendientes bastardos) Relación dirigida a anotar en un solo sentido, cuando no se conoce la cantidad de generaciones que separan a dos individuos ligados por linaje bastardo</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="ascendientes_bastardos">
                     <xs:annotation>
                        <xs:documentation>(Relación de ascendientes a descendientes bastardos) Relación dirigida a anotar en un solo sentido, cuando no se conoce la cantidad de generaciones que separan a dos individuos ligados por linaje bastardo</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="descendientes_indeterminados">
                     <xs:annotation>
                        <xs:documentation>(Relación de descendientes a ascendientes indeterminados) Relación dirigida a anotar en un solo sentido</xs:documentation>
                     </xs:annotation>
                  </xs:enumeration>
                  <xs:enumeration value="ascendientes_indeterminados">
                     <xs:annotation>
